{
	"welcomeFile": "/vehiclecatalogue/index.html",
	"authenticationMethod": "route",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"routes": [{
		"source": "^/vehiclecatalogue/(.*)$",
		"target": "$1",
		"localDir": "webapp"
	}, {
		"source": "/node(.*)",
		"destination": "vehiclecatalogue-node",
		"csrfProtection": false
	}, {
		"source": "/userDetails(.*)",
		"destination": "vehiclecatalogue-node",
		"csrfProtection": false
	}]
}