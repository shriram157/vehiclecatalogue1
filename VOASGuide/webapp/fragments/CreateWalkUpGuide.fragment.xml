<core:FragmentDefinition xmlns:core="sap.ui.core"
	controllerName="com.sap.build.toyota-canada.vehiclesGuideV3.controller.util.CreateWalkUpGuide" xmlns="sap.m" xmlns:form="sap.ui.layout.form">
	<Dialog horizontalScrolling="true" showHeader="true" state="None" stretch="false" title="Create Walk Up Guide" type="Standard"
		verticalScrolling="true">
		<content>
			<form:Form width="589.046875px" editable="true" visible="true" class="sapUiTinyMargin">
				<form:title/>
				<form:formContainers>
					<form:FormContainer visible="true">
						<form:formElements>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Language" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" />
								</form:label>
									<form:fields>
									<Switch state="true" customTextOff="En" customTextOn="Fr" type="Default" enabled="true" visible="true" class="sapUiTinyMargin"/>
								</form:fields>
								<form:fields>
									<ComboBox id="idWalk_brandCB" placeholder="Select Brand" editable="true" enabled="true" visible="true" width="400px" valueState="None" maxWidth="100%"
										class="sapUiResponsiveMargin" items="{path:'brandModelNew>/modelBrand', templateShareable:true}">
										<items>
												<core:Item key="{brandModelNew>key}" text="{brandModelNew>text}"/>
										</items>
									</ComboBox>
									<ComboBox id="idWalk_modelYearCB" change="onChange_ModelYear" placeholder="Select Year" editable="true" enabled="true" visible="true" width="398px" valueState="None" maxWidth="100%"
										class="sapUiResponsiveMargin" items="{path:'yearModelNew>/modelYear', templateShareable:true}">
										<items>
											<core:Item key="{yearModelNew>key}" enabled="true" text="{yearModelNew>text}"/>
										</items>
									</ComboBox>
									<ComboBox id="idWalk_seriesCB" placeholder="Select Series" editable="true" enabled="true" visible="true" width="397px" valueState="None" maxWidth="100%"
										class="sapUiResponsiveMargin" items="{path:'seriesdropDownModelNew>/', templateShareable:true}">
										<items>
											<core:Item text="{seriesdropDownModelNew>}" enabled="true" key="{seriesdropDownModelNew>}"/>
										</items>
									</ComboBox>
								</form:fields>
							</form:FormElement>
						</form:formElements>
						<form:title/>
					</form:FormContainer>
				</form:formContainers>
				<form:layout>
					<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="1" labelSpanL="12" columnsM="1" labelSpanM="6" labelSpanS="12"/>
				</form:layout>
			</form:Form>
		</content>
		<beginButton/>
		<buttons>
			<Button text="Create PDF" type="Default" icon="sap-icon://pdf-attachment" iconFirst="true" width="auto" enabled="true" visible="true"
				iconDensityAware="false" press="_onButtonPress"/>
			<Button text="Close" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"
				press="_onButtonPress1"/>
		</buttons>
		<customHeader/>
		<endButton/>
		<subHeader/>
	</Dialog>
</core:FragmentDefinition>