<core:FragmentDefinition id="supp_fragmentId" xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:form="sap.ui.layout.form" xmlns:u="sap.ui.unified" controllerName="com.sap.build.toyota-canada.vehiclesGuideV3.controller.util.SuplementalDialog"> 
	<!--controllerName="com.sap.build.toyota-canada.vehiclesGuideV3.controller.util.SuplementalDialog" -->
	<Dialog horizontalScrolling="true" showHeader="true" state="None" stretch="false" title="Supplemental Section PDF" type="Standard"
		verticalScrolling="true">
			<content>
			<form:Form width="100%" editable="true" visible="true">
				<form:title/>
				<form:formContainers>
					<form:FormContainer visible="true">
						<form:formElements>
							<form:FormElement visible="true">
								<form:label>
									<Label text="" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<!--<Button text="Upload PDF" type="Default" icon="sap-icon://upload" iconFirst="true" width="auto" enabled="true" visible="true"
										iconDensityAware="false"/>-->
									<u:FileUploader id="suppFileUploadId" name="myFileUpload" uploadUrl="upload/" tooltip="Upload your file to the server"
										uploadComplete="handleUploadComplete"/>
								</form:fields>
							</form:FormElement>
							<!--	<form:FormElement visible="true">
								<form:label>
									<Label text="Effective Date" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<DatePicker width="auto" displayFormat="medium" enabled="true" visible="true" valueFormat="yyyyMMdd"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Expiry Date" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<DatePicker width="auto" displayFormat="medium" enabled="true" visible="true" valueFormat="yyyyMMdd"/>
								</form:fields>
							</form:FormElement>-->
							<form:FormElement visible="true">
								<form:label>
									<Label text="Comments" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<TextArea id="Supp_TA_Comment" rows="2" cols="20" maxLength="0" wrapping="None" enabled="true" visible="true"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true" class="sapUiResponsiveMargin">
								<form:label>
									<Label text="Language" design="Bold" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<Switch id="Supp_Switch_Lang" state="true" customTextOff="En" customTextOn="Fr" type="Default" enabled="true" visible="true" class="sapUiTinyMargin"/>
								</form:fields>
							</form:FormElement>
						</form:formElements>
						<form:title/>
					</form:FormContainer>
				</form:formContainers>
				<form:layout>
					<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
				</form:layout>
			</form:Form>
		</content>
		<beginButton/>
		<buttons>
			<Button text="Upload PDF" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"
				press="_uploadPDFFile"/>
			<Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"
				press="_onButtonPress1"/>
		</buttons>
		<customHeader/>
		<endButton/>
		<subHeader/>
	</Dialog>
</core:FragmentDefinition>