<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.toyota-canada.vehiclesGuideV3.controller.Page5" xmlns="sap.m"
	xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:layout1="sap.ui.layout">
	<Page showHeader="true" title="Create Vehicle Guide" showFooter="false" showNavButton="false">
		<content>
			<form:Form width="100%" editable="true" visible="true">
				<form:title/>
				<form:formContainers>
					<form:FormContainer visible="true">
						<form:formElements>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Include Dealer Net" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Switch state="false" customTextOff="Off" customTextOn="On" type="Default" enabled="true" visible="true"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true" class="sapUiResponsiveMargin">
								<form:label>
									<Label text="Brand" design="Bold" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<ComboBox placeholder="Select" editable="true" enabled="true" visible="true" width="auto" valueState="None" maxWidth="100%"
										items="{path:'/VehicleSet', templateShareable:true}">
										<items>
											<core:Item text="{Brand}" enabled="true" key="{Brand}"/>
										</items>
									</ComboBox>
									<Label text="Year" design="Standard" width="128px" required="true" textAlign="Right" textDirection="Inherit" visible="true"
										class="sapUiResponsiveMargin"/>
									<ComboBox placeholder="Select" editable="true" enabled="true" visible="true" width="auto" valueState="None" maxWidth="100%"
										items="{path:'/VehicleSet', templateShareable:true}">
										<items>
											<core:Item text="{ModelYear}" enabled="true" key="{ModelYear}"/>
										</items>
									</ComboBox>
									<Label text="Series" design="Standard" width="126px" required="true" textAlign="Right" textDirection="Inherit" visible="true"
										class="sapUiResponsiveMargin"/>
									<ComboBox selectedKey="" placeholder="Select" editable="true" enabled="true" visible="true" width="auto" valueState="None" maxWidth="100%"
										items="{path:'/VehicleSet', templateShareable:true}">
										<items>
											<core:Item text="{Series}" enabled="true" key="{Series}"/>
										</items>
									</ComboBox>
								</form:fields>
							</form:FormElement>
						</form:formElements>
						<form:title/>
					</form:FormContainer>
				</form:formContainers>
				<form:layout>
					<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
				</form:layout>
			</form:Form>
			<layout1:Grid defaultIndent="L0 M0 S0" defaultSpan="L5 M6 S12" position="Left" width="100%" containerQuery="false" hSpacing="1" vSpacing="1"
				visible="true">
				<layout1:content>
					<Text text="" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
					<Button text="Create PDF Guide" type="Default" icon="sap-icon://pdf-attachment" iconFirst="true" width="191px" enabled="true" visible="true"
						iconDensityAware="false"/>
					<Text text="" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
					<Text text="" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
					<Text text="" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
					<Text text="" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
				</layout1:content>
			</layout1:Grid>
		</content>
		<footer>
			<OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
				<content>
					<ToolbarSpacer width=""/>
					<Button text="Save" type="Emphasized" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
					<Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
				</content>
			</OverflowToolbar>
		</footer>
		<headerContent/>
		<subHeader/>
		<customHeader/>
	</Page>
</mvc:View>