<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.toyota-canada.vehiclesGuideV3.controller.SearchPage" xmlns:f="sap.f"
	xmlns:variants="sap.ui.comp.variants" xmlns="sap.m" xmlns:filterbar="sap.ui.comp.filterbar" xmlns:core="sap.ui.core">
	<f:DynamicPage headerExpanded="true" showFooter="true" floatingFooter="false" class="sapUiSizeCompact">
		<f:title>
			<f:DynamicPageTitle>
				<!--<f:heading>
					<Image id="idLexusLogo" src="images/Lexus_EN.png" width="100px"/>
					</f:heading>-->
				<f:actions>
					<ToolbarSpacer/>
					<!--	<variants:VariantManagement/>-->
					<!--<Button icon="sap-icon://action" type="Transparent"/>
					{=${SeriesData>SPRAS}=='EN' ? ${SeriesData>TCISeriesDescriptionEN}:${SeriesData>TCISeriesDescriptionFR}-->
				</f:actions>
			</f:DynamicPageTitle>
		</f:title>
		<f:header>
			<f:DynamicPageHeader pinnable="true">
				<f:content>
					<filterbar:FilterBar id="filterBar" useToolbar="false" showGoOnFB="false" search="pressGo" searchEnabled="true"
						showFilterConfiguration="false">
						<filterbar:filterItems>
							<filterbar:FilterItem label="{i18n>Brand}"
								name="Fiori_ListReport_ListReport_0-filterBars-Fiori_ListReport_FilterBar-1-filters-Fiori_ListReport_ComboBoxFilter-1538670414396---0">
								<filterbar:control>
									<ComboBox id="id_brandCB" enabled="true" change="onChange_Brand" items="{path:'brandModel>/modelBrand', templateShareable:true}">
										<items>
											<core:Item key="{brandModel>key}" text="{brandModel>text}"/>
										</items>
									</ComboBox>
								</filterbar:control>
							</filterbar:FilterItem>
							<filterbar:FilterItem label="{i18n>ModelYear}"
								name="Fiori_ListReport_ListReport_0-filterBars-Fiori_ListReport_FilterBar-1-filters-Fiori_ListReport_ComboBoxFilter-1538673647772---0">
								<filterbar:control>
									<ComboBox id="id_modelYearCB" change="onChange_ModelYear" enabled="false" items="{path:'yearModel>/modelYear', templateShareable:true}">
										<items>
											<core:Item key="{yearModel>key}" text="{yearModel>text}"/>
										</items>
									</ComboBox>
								</filterbar:control>
							</filterbar:FilterItem>
							<filterbar:FilterItem label="{i18n>Series}"
								name="Fiori_ListReport_ListReport_0-filterBars-Fiori_ListReport_FilterBar-1-filters-Fiori_ListReport_ComboBoxFilter-1538673653531---0">
								<filterbar:control>
									<ComboBox id="id_seriesCB" change="onChange_Series" enabled="false" items="{path:'seriesdropDownModel>/', templateShareable:true}">
										<items>
											<core:Item key="{seriesdropDownModel>}" text="{seriesdropDownModel>}"/>
										</items>
									</ComboBox>
								</filterbar:control>
							</filterbar:FilterItem>
							<filterbar:FilterItem label="{i18n>Model}"
								name="Fiori_ListReport_ListReport_0-filterBars-Fiori_ListReport_FilterBar-1-filters-Fiori_ListReport_ComboBoxFilter-1538673654522---0">
								<filterbar:control>
									<MultiComboBox id="id_modelCB" selectionFinish="onChange_Model" enabled="false" items="{path:'dropDownModel>/', templateShareable:true}">
										<items>
											<core:Item key="{dropDownModel>}" text="{dropDownModel>}"/>
										</items>
									</MultiComboBox>
								</filterbar:control>
							</filterbar:FilterItem>
							<filterbar:FilterItem label="{i18n>Suffix}"
								name="Fiori_ListReport_ListReport_0-filterBars-Fiori_ListReport_FilterBar-1-filters-Fiori_ListReport_ComboBoxFilter-1538673656505---0">
								<filterbar:control>
									<MultiComboBox id="id_suffixCB" enabled="false" selectionFinish="onChange_Suffix"
										items="{path:'suffixdropDownModel>/', templateShareable:true}">
										<!--selectionFinish="onChange_Suffix" -->
										<items>
											<core:Item key="{suffixdropDownModel>}" text="{suffixdropDownModel>}"/>
											<!--text="{parts: [{path:'suffixdropDownModel>'},{path:'suffixdropDownModel>'}] , formatter: 'formatter.formatSuffixInList'}"  -->
										</items>
									</MultiComboBox>
								</filterbar:control>
							</filterbar:FilterItem>
							<!--<filterbar:FilterItem label="" name="Fiori_ListReport_ListReport_0_go">
								<filterbar:control>
									<Button text="Go"></Button>
								</filterbar:control>
							</filterbar:FilterItem>-->
						</filterbar:filterItems>
					</filterbar:FilterBar>
				</f:content>
			</f:DynamicPageHeader>
		</f:header>
		<f:content>
			<VBox fitContainer="true">
				<items>
					<OverflowToolbar height="46px" design="Solid" visible="true" enabled="true">
						<content>
							<ToolbarSpacer width=""/>
							<Button text="{i18n>WhatsNew}" type="Default" icon="sap-icon://pdf-attachment" iconFirst="true" enabled="true" visible="true"
								iconDensityAware="false" press="_onCreateWhatsNew"/>
							<Button text="{i18n>WalkUp}" type="Default" icon="sap-icon://pdf-attachment" iconFirst="true" enabled="true" visible="true"
								iconDensityAware="false" press="_onCreateWalkUp"/>
							<Button text="{i18n>Supplemental}" type="Default" icon="sap-icon://pdf-attachment" iconFirst="true" enabled="true" visible="true"
								iconDensityAware="false" press="_onCreateSupplemental"/>
							<Button text="{i18n>VehicleGuide}" type="Default" icon="sap-icon://pdf-attachment" iconFirst="true" enabled="true" visible="true"
								iconDensityAware="false" press="_onCreateVehGuide"/>
							<Button text="{i18n>PocketSummary}" type="Default" icon="sap-icon://pdf-attachment" iconFirst="true" enabled="true" visible="true"
								iconDensityAware="false" press="_onCreatePocSum"/>
						</content>
					</OverflowToolbar>
					<Table id="idTbl_Search" width="100%" growing="true" growingScrollToLoad="true" growingThreshold="25" mode="MultiSelect"
						itemPress="_navToDetail" updateFinished="_onSearchTableUpdate" items="{path:'searchTblModel>/', templateShareable:true}"
						class="marginCornersBottom">
						<headerToolbar>
							<OverflowToolbar design="Transparent">
								<content>
									<Title textAlign="Begin" text="{i18n>VehOrdGuideSearchText}"/>
									<Title id="id_SP_noOfData" text="(0)"/>
									<ToolbarSpacer/>
									<Button text="{i18n>Compare}" icon="sap-icon://compare" iconFirst="true" type="Transparent" enabled="true" press="_navToCompare">
										<layoutData>
											<OverflowToolbarLayoutData/>
										</layoutData>
									</Button>
								</content>
							</OverflowToolbar>
						</headerToolbar>
						<columns>
							<Column minScreenWidth="Phone" width="25%">
								<header>
									<Text text="{i18n>Vehicle}"/>
								</header>
							</Column>
							<Column minScreenWidth="Phone" width="50%">
								<header>
									<Text text="{i18n>Features}"/>
								</header>
							</Column>
							<Column id="priceColTblIdSearch" visible="true" minScreenWidth="Phone" width="25%">
								<header>
									<Text text="{i18n>Price}"/>
								</header>
							</Column>
						</columns>
						<items>
							<ColumnListItem type="Navigation">
								<cells>
									<ObjectIdentifier
										text="{parts: [{path:'searchTblModel>Modelyear'},
										{path:'searchTblModel>Model'},
										{path:'searchTblModel>ENModelDesc'},{path:'searchTblModel>FRModelDesc'},
										{path:'searchTblModel>Suffix'},
										{path:'searchTblModel>suffix_desc_en'},{path:'searchTblModel>suffix_desc_fr'},{path:'searchTblModel>Language'}] ,
										formatter: 'formatter.formatVehicle'}"/>
									<!--title="{searchTblModel>Vehicle}" text="{parts: [{path:'searchTblModel>Suffix'},{path:'searchTblModel>Suffix'}] , formatter: 'formatter.formatSuffix'}"/>-->
									<!-- text="SFX- {searchTblModel>Suffix}"/>-->
									<ObjectIdentifier
										text="{parts: [{path:'searchTblModel>Features'},{path:'searchTblModel>Features'}] , formatter: 'formatter.formatFeatures'}"/>
									<!--				<ObjectIdentifier text="Dealer Net: {searchTblModel>NETPRICE}" title="MSRP: {searchTblModel>MSRP}"/>-->
									<ObjectIdentifier
										text="{i18n>DealerNet}{parts: [{path:'searchTblModel>NETPRICE'},{path:'searchTblModel>NETPRICE'}] , formatter: 'formatter.decimalFormatterDealer'}"
										title="{i18n>MSRP}{parts: [{path:'searchTblModel>MSRP'},{path:'searchTblModel>MSRP'}] , formatter: 'formatter.decimalFormatter'}"/>
								</cells>
							</ColumnListItem>
						</items>
					</Table>
					<!--<OverflowToolbar height="46px" design="Solid" visible="true" enabled="true">
						<content>
							<Text text="Disclaimer:" class="sapUiMediumMarginBegin"></Text>
						</content>
					</OverflowToolbar>-->
				</items>
			</VBox>
		</f:content>
		<!--<f:footer id="foot">
			<OverflowToolbar height="46px" design="Solid" visible="true" enabled="true">
				<content>
					<Text text="Disclaimer:" class="sapUiMediumMarginBegin"></Text>
				</content>
			</OverflowToolbar>
		</f:footer>-->
	</f:DynamicPage>
</mvc:View>