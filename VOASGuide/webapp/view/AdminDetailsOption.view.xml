<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.toyota-canada.vehiclesGuideV3.controller.AdminDetailsOption"
	xmlns:uxap="sap.uxap" xmlns="sap.m">
	<uxap:ObjectPageLayout showTitleInHeaderContent="false" showHeaderContent="true" showEditHeaderButton="false" showFooter="false"
		class="sapUiSizeCompact">
		<uxap:headerTitle>
			<uxap:ObjectPageHeader objectTitle="2019 Toyota Camry" objectSubtitle="" isObjectIconAlwaysVisible="false" isObjectTitleAlwaysVisible="true"
				isObjectSubtitleAlwaysVisible="true" showPlaceholder="true" objectImageURI="/build/uilibraries/content/static/image-canvas.svg">
				<uxap:navigationBar>
					<Bar>
						<contentLeft>
							<Button type="Back" press="onNavBack"/>
						</contentLeft>
						<contentMiddle>
							<Title text="Manage Vehicle Series Details"/>
						</contentMiddle>
						<contentRight/>
					</Bar>
				</uxap:navigationBar>
				<uxap:actions>
					<Button text="Create Vehicle Guide" icon="sap-icon://pdf-attachment" iconFirst="false" type="Default" enabled="true"
						press="_createVehGuidePDF">
						<layoutData>
							<OverflowToolbarLayoutData moveToOverflow="true"/>
						</layoutData>
					</Button>
				</uxap:actions>
				<uxap:breadCrumbsLinks/>
			</uxap:ObjectPageHeader>
		</uxap:headerTitle>
		<uxap:headerContent/>
		<uxap:sections>
			<uxap:ObjectPageSection title="What's New">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="What's New">
						<uxap:blocks>
							<Table width="100%" growing="true" growingScrollToLoad="true" growingThreshold="25" mode="SingleSelectLeft"
								items="{path:'/WhatsNewAdminSet', templateShareable:true}">
								<headerToolbar>
									<OverflowToolbar design="Transparent">
										<content>
											<ToolbarSpacer/>
											<Button text="Upload" icon="sap-icon://write-new" iconFirst="true" type="Transparent" press="_uploadWhatNew">
												<layoutData>
													<OverflowToolbarLayoutData moveToOverflow="true"/>
												</layoutData>
											</Button>
											<Button text="Delete" icon="sap-icon://delete" iconFirst="false" type="Transparent" press="_deleteWhatNew">
												<layoutData>
													<OverflowToolbarLayoutData moveToOverflow="true"/>
												</layoutData>
											</Button>
											<!--<Button icon="sap-icon://action-settings" type="Transparent">
												<layoutData>
													<OverflowToolbarLayoutData moveToOverflow="true"/>
												</layoutData>
											</Button>-->
										</content>
									</OverflowToolbar>
								</headerToolbar>
								<columns>
									<Column width="auto" minScreenWidth="Phone">
										<header>
											<Text text="File Name" width="auto"/>
										</header>
									</Column>
									<Column width="auto" minScreenWidth="Tablet" demandPopin="true">
										<header>
											<Text text="Comments" width="auto"/>
										</header>
									</Column>
									<Column width="auto" minScreenWidth="Phone">
										<header>
											<Text text="Last Uploaded" width="auto"/>
										</header>
									</Column>
									<Column width="auto" minScreenWidth="Phone">
										<header>
											<Text text="Language" width="auto"/>
										</header>
									</Column>
								</columns>
								<items>
									<ColumnListItem type="Active">
										<cells>
											<Link text="{filename}" href="http://www.build.me" target="_blank"/>
											<ObjectStatus text="{comment}"/>
											<ObjectIdentifier title="{date}" text="{EndDateDetails}"/>
											<ObjectIdentifier title="{lang}" text="{EndDateDetails}"/>
										</cells>
									</ColumnListItem>
								</items>
							</Table>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>
			<uxap:ObjectPageSection title="Walk-up">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="Walk-up">
						<uxap:blocks>
							<Table width="100%" growing="false" growingScrollToLoad="true" growingThreshold="5" mode="SingleSelectLeft"
								items="{path:'/WalkupAdminSet', templateShareable:true}">
								<headerToolbar>
									<OverflowToolbar design="Transparent">
										<content>
											<ToolbarSpacer/>
											<Button text="Upload" icon="sap-icon://write-new" iconFirst="true" type="Transparent" press="_uploadFileWalkUp">
												<layoutData>
													<OverflowToolbarLayoutData moveToOverflow="true"/>
												</layoutData>
											</Button>
											<Button text="Delete" icon="sap-icon://delete" iconFirst="false" type="Transparent" press="_deleteFileWalkUp">
												<layoutData>
													<OverflowToolbarLayoutData moveToOverflow="true"/>
												</layoutData>
											</Button>
											<!--<Button icon="sap-icon://action-settings" type="Transparent">
												<layoutData>
													<OverflowToolbarLayoutData moveToOverflow="true"/>
												</layoutData>
											</Button>-->
										</content>
									</OverflowToolbar>
								</headerToolbar>
								<columns>
									<Column width="auto" minScreenWidth="Phone">
										<header>
											<Text text="File Name" width="auto"/>
										</header>
									</Column>
									<Column width="auto" minScreenWidth="Tablet" demandPopin="true">
										<header>
											<Text text="Comments" width="auto"/>
										</header>
									</Column>
									<Column width="auto" minScreenWidth="Phone">
										<header>
											<Text text="Last Uploaded" width="auto"/>
										</header>
									</Column>
									<Column width="auto" minScreenWidth="Phone">
										<header>
											<Text text="Language" width="auto"/>
										</header>
									</Column>
								</columns>
								<items>
									<ColumnListItem type="Active">
										<cells>
											<Link text="{filename}" href="http://www.build.me" target="_blank"/>
											<ObjectStatus text="{comment}"/>
											<ObjectIdentifier title="{date}" text="{EndDateDetails}"/>
											<ObjectIdentifier title="{lang}" text="{EndDateDetails}"/>
										</cells>
									</ColumnListItem>
								</items>
							</Table>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>
			<uxap:ObjectPageSection title="Supplement Guide">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="Supplement Guide">
						<uxap:blocks>
							<Table id="suppTbl" width="100%" growing="true" growingScrollToLoad="true" growingThreshold="25" mode="SingleSelectLeft"
								items="{path:'/', templateShareable:true}">
								<headerToolbar>
									<OverflowToolbar design="Transparent">
										<content>
											<ToolbarSpacer/>
											<Button text="Upload" icon="sap-icon://write-new" iconFirst="true" type="Transparent" press="_uploadFileSupp">
												<layoutData>
													<OverflowToolbarLayoutData moveToOverflow="true"/>
												</layoutData>
											</Button>
											<Button text="Delete" icon="sap-icon://edit" iconFirst="false" type="Transparent" press="_deleteFileSupp">
												<layoutData>
													<OverflowToolbarLayoutData moveToOverflow="true"/>
												</layoutData>
											</Button>
											<!--<Button icon="sap-icon://action-settings" type="Transparent">
												<layoutData>
													<OverflowToolbarLayoutData moveToOverflow="true"/>
												</layoutData>
											</Button>-->
										</content>
									</OverflowToolbar>
								</headerToolbar>
								<columns>
									<Column width="auto" minScreenWidth="Phone">
										<header>
											<Text text="File Name" width="auto"/>
										</header>
									</Column>
									<Column width="auto" minScreenWidth="Tablet" demandPopin="true">
										<header>
											<Text text="Comments" width="auto"/>
										</header>
									</Column>
									<Column width="auto" minScreenWidth="Phone">
										<header>
											<Text text="Last Uploaded" width="auto"/>
										</header>
									</Column>
									<Column width="auto" minScreenWidth="Phone">
										<header>
											<Text text="Language" width="auto"/>
										</header>
									</Column>
								</columns>
								<items>
									<ColumnListItem type="Active">
										<cells>
											<Link text="{filename}" href="http://www.build.me" target="_blank"/>
											<ObjectStatus text="{comment}"/>
											<ObjectIdentifier title="{date}" text="{EndDateDetails}"/>
											<ObjectIdentifier title="{lang}" text="{EndDateDetails}"/>
										</cells>
									</ColumnListItem>
								</items>
							</Table>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>
		</uxap:sections>
		<uxap:footer>
			<OverflowToolbar>
				<content>
					<ToolbarSpacer/>
					<Button text="Save" type="Emphasized" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
					<Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
				</content>
			</OverflowToolbar>
		</uxap:footer>
	</uxap:ObjectPageLayout>
</mvc:View>