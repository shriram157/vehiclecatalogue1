<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" controllerName="com.sap.build.toyota-canada.vehiclesGuideV3.controller.DetailsOption" xmlns:uxap="sap.uxap">
	<uxap:ObjectPageLayout class="sapUiSizeCompact" showTitleInHeaderContent="false" showHeaderContent="true" showEditHeaderButton="false" showFooter="false">
		<uxap:headerTitle>
			<uxap:ObjectPageHeader objectTitle="{modelDetail>/moYear} {modelDetail&gt;/brand} {modelDetail>/series} - {i18n>SeeAllFeatures}" isObjectIconAlwaysVisible="false" isObjectTitleAlwaysVisible="true" isObjectSubtitleAlwaysVisible="true" showPlaceholder="true">
				<!--objectImageURI="/build/uilibraries/content/static/image-canvas.svg"-->
				<uxap:navigationBar>
					<Bar xmlns="sap.m">
						<contentLeft>
							<Button type="Back" text="Back" press="_onFioriObjectPageHeaderPress"/>
						</contentLeft>
						<contentMiddle>
							<Title text="{i18n>VehicleDetails}"/>
						</contentMiddle>
						<contentRight/>
					</Bar>
				</uxap:navigationBar>
				<uxap:actions> <!--	press="_onFioriObjectPageActionButtonPress"-->
					<Button xmlns="sap.m" type="Default" text="{i18n>VehicleGuide}" enabled="true" press="_onCreateVehGuide" iconFirst="true" icon="sap-icon://pdf-attachment"/>
					<Button xmlns="sap.m" type="Default" text="{i18n>WhatsNewGuide}" enabled="true" press="_onCreateWhatsNew" iconFirst="true" icon="sap-icon://pdf-attachment"/>
					<Button xmlns="sap.m" type="Default" text="{i18n>WalkUpGuide}" enabled="true" press="_onCreateWalkUp" iconFirst="true" icon="sap-icon://pdf-attachment"/>
					<Button xmlns="sap.m" type="Default" text="{i18n>Supplemental}" enabled="true" press="_onCreateSupplemental" iconFirst="true" icon="sap-icon://pdf-attachment"/>
					<Button xmlns="sap.m" type="Default" text="{i18n>PocketSummary}" enabled="true" press="_onCreatePocSum" iconFirst="true" icon="sap-icon://pdf-attachment" iconDensityAware="false" visible="true"/>
					<Button xmlns="sap.m" id="manageSeriesBtnId" type="Transparent" text="{i18n>ManageSeries}" enabled="false" press="manageSeries" iconFirst="true" icon="sap-icon://edit"/>
				</uxap:actions>
				<uxap:breadCrumbsLinks/>
			</uxap:ObjectPageHeader>
		</uxap:headerTitle>
		<uxap:headerContent>
			<VBox xmlns="sap.m" width="100%">
				<HBox class="justifyCenter">
					<VBox>
						<items>
							<Title text="{modelDetail>/ENModelDesc} - ({modelDetail>/model}) - SFX {modelDetail>/suffix}"/>
						<!--	<Title text="{{=${modelDetail>lang}=='EN' ? ${modelDetail>ENModelDesc}:${modelDetail>FRModelDesc}} - ({modelDetail>/model}) - SFX {modelDetail>/suffix}"/>-->
							<HBox>
								<items>
									<Label text="{i18n>Model}"/>
									<Label class="sapUiTinyMarginEnd" text=":"/>
									<!--	<ObjectStatus text="{modelDetail>/model}"/>-->
									<ObjectStatus text="{modelDetail>/ENModelDesc}"/>
								</items>
							</HBox>
							<HBox>
								<items>
									<!--<Label text="MSRP"/>
							<Label text=":" class="sapUiTinyMarginEnd"/>
							<ObjectStatus text="{modelDetail>/msrp} "/>-->
									<Label text="{i18n>MSRPWithoutDoll}"/>
									<ObjectStatus text="{parts: [{path:'modelDetail>/msrp'},{path:'modelDetail&gt;/msrp'}] , formatter: 'formatter.decimalFormatter'}"/>
								</items>
							</HBox>
							<HBox>
								<items>
									<!--<Label text="Dealer Net"/>
									<Label text=":" class="sapUiTinyMarginEnd"/>
									<ObjectStatus text="{modelDetail>/dealerNet}"/>-->
										<Label text="{i18n>DealerNetWithoutDoll}"/>
									<ObjectStatus text="{parts: [{path:'modelDetail>/dealerNet'},{path:'modelDetail&gt;/dealerNet'}] , formatter: 'formatter.decimalFormatterDealer'}"/>
								</items>
							</HBox>
						</items>
					</VBox>
				</HBox>
			</VBox>
		</uxap:headerContent>
		<uxap:sections>
			<uxap:ObjectPageSection title="Powertrain &amp; Mechanical">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="{i18n>Powertrain}">
						<uxap:blocks>
							<Table xmlns="sap.m" id="DetailtblpwrTrn" width="100%" noDataText=" " growing="false" growingScrollToLoad="true" growingThreshold="30" mode="None"/>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>
			<uxap:ObjectPageSection title="Safety &amp; Convenience">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="{i18n>Safety}">
						<uxap:blocks>
							<Table xmlns="sap.m" id="DetailtblSafety" width="100%" noDataText=" " growing="false" growingScrollToLoad="true" growingThreshold="30" mode="None"/>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>
			<uxap:ObjectPageSection title="Exterior">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="{i18n>Exterior}">
						<uxap:blocks>
							<Table xmlns="sap.m" id="DetailtblExterior" width="100%" noDataText=" " growing="false" growingScrollToLoad="true" growingThreshold="30" mode="None"/>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>
			<uxap:ObjectPageSection title="Interior">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="{i18n>Interior}">
						<uxap:blocks>
							<Table xmlns="sap.m" id="DetailtblInterior" width="100%" noDataText=" " growing="false" growingScrollToLoad="true" growingThreshold="30" mode="None"/>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>
		
			
			
			<uxap:ObjectPageSection title="Infotainment">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="{i18n>Infotainment}">
						<uxap:blocks>
							<Table xmlns="sap.m" id="DetailtblInfotainment" width="100%" noDataText=" " growing="false" growingScrollToLoad="true" growingThreshold="30" mode="None"/>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>
			<uxap:ObjectPageSection title="Dimensions &amp; Specs">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="{i18n>DimensionsSpecs}">
						<uxap:blocks>
							<Table xmlns="sap.m" id="DetailtblDimensions" width="100%" noDataText=" " growing="false" growingScrollToLoad="true" growingThreshold="30" mode="None"/>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>
			<uxap:ObjectPageSection title="Color Options">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="{i18n>ColorOptions}">
						<uxap:blocks>
							<Table xmlns="sap.m" id="DetailtblColorOptions" width="100%" noDataText=" " growing="false" growingScrollToLoad="true" growingThreshold="30" mode="None"/>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>
			
			<uxap:ObjectPageSection title="Option Packages">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="{i18n>OptionPackages}">
						<uxap:blocks>
							<Table xmlns="sap.m" id="DetailtblOptionPack" width="100%" noDataText=" " growing="false" growingScrollToLoad="true" growingThreshold="30" mode="None"/>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>
			<uxap:ObjectPageSection title="={i18n>APX}">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="{i18n>APX}">
						<uxap:blocks>
							<VBox xmlns="sap.m">
								<Table id="DetailtblAPX1" width="100%" noDataText=" " growing="false" growingScrollToLoad="true" growingThreshold="30" mode="None"/>
							<Table id="DetailtblAPX" width="100%" noDataText=" " growing="false" growingScrollToLoad="true" growingThreshold="30" mode="None"/>
							</VBox>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>
				<uxap:ObjectPageSection title="">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="">
						<uxap:blocks>
							<VBox xmlns="sap.m">
								 <!--<TextArea id="tADetailDisclaimer" value=" * CONFIDENTIALITY NOTICE -This document is intended for Lexus employees and Lexus Dealer staff only.   Redistribution in any form is not permitted without express written authorization from the Lexus Division of Toyota Canada Inc.  **Every effort has been made to ensure the specifications and equipment shown are accurate based on information available at time of printing. The Owner’s Manual remains the most accurate source of technical specification on our vehicles. Lexus reserves the right to make necessary changes without notice or obligation." height="100%" rows="4" cols="175"/>-->
